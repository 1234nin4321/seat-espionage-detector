{
    "name": "1234nin4321/seat-espionage-detector",
    "description": "SeAT 5 plugin for detecting potential spies through ESI analysis",
    "type": "seat-plugin",
    "license": "MIT",
	"minimum-stability": "stable",
	"prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "eveseat/eveapi": "^5.0.1",
        "eveseat/services": "^5.0.1",
        "eveseat/web": "^5.0.1",
        "recursivetree/seat-treelib": "^2.1.3",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "1234nin4321\\Seat\\EspionageDetector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "1234nin4321\\Seat\\EspionageDetector\\EspionageDetectorServiceProvider"
            ]
        }
    }
}